<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE logback>
<configuration>
    <appender name = "STDOUT" class = "ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{HH:mm:ss:SSS} [%thread] %-5level %logger{15}#%line %msg\n</pattern>
        </encoder>
    </appender>

    <logger name = "pro.lisanail">
        <>
    </logger>
</configuration>